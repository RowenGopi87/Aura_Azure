# Copy these lines to your .env file for full RAG functionality

# Database Configuration (already configured)
AURA_DB_HOST=127.0.0.1
AURA_DB_PORT=3306
AURA_DB_USER=aura_user
AURA_DB_PASSWORD=aura_password_123
AURA_DB_NAME=aura_playground
AURA_DB_MAX_POOL_SIZE=10
AURA_DB_SSL=false

# RAG & Embedding Configuration (ADD THESE TO YOUR .env FILE)
AURA_EMBEDDING_PROVIDER=openai
AURA_EMBEDDING_API_KEY=your_openai_api_key_here
AURA_EMBEDDING_MODEL=text-embedding-3-small

# Alternative: Use OpenAI directly
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: Other embedding providers
# AURA_EMBEDDING_PROVIDER=gemini
# AURA_EMBEDDING_API_KEY=your_gemini_api_key

# AURA_EMBEDDING_PROVIDER=huggingface  
# AURA_EMBEDDING_API_KEY=your_huggingface_token
